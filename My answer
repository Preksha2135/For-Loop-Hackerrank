#include<iostream>
using namespace std;



int main() {
    int n,m;
    cin>>n>>m;
    

        if(n>=1&&n<=9){
            switch(n){
                case 1:
                cout<<"one \n";
                break;
                case 2:
                cout<<"two \n";
                break;
                case 3:
                cout<<"three \n";
                break;
                case 4:
                cout<<"four \n";
                break;
                case 5:
                cout<<"five \n";
                break;
                case 6:
                cout<<"six \n";
                break;
                case 7:
                cout<<"seven \n";
                break;
                case 8:
                cout<<"eight \n";
                break;
                default:
                cout<<"nine \n";
                break;
            }
        }
    
        else if(n>9&&n%2==0) {
        cout<<"even \n";
        }
        else {
        cout<<"odd \n";
        }
    
        if(m>=1&&m<=9){
            switch(m){
                case 1:
                cout<<"one \n";
                break;
                case 2:
                cout<<"two \n";
                break;
                case 3:
                cout<<"three \n";
                break;
                case 4:
                cout<<"four \n";
                break;
                case 5:
                cout<<"five \n";
                break;
                case 6:
                cout<<"six \n";
                break;
                case 7:
                cout<<"seven \n";
                break;
                case 8:
                cout<<"eight \n";
                break;
                default:
                cout<<"nine \n";
                break;
            }
        }
    
        else if(m>9&&m%2==0) {
        cout<<"even \n";
        }
        else {
        cout<<"odd \n";
        }
    return 0;
        
}
